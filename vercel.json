{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/swagger-ui/(.*)",
      "dest": "/public/swagger-ui/$1"
    },
    {
      "src": "/swagger-ui.css",
      "dest": "/public/swagger-ui.css"
    },
    { "src": "/(.*)", "dest": "dist/main.js" }
  ]
}
